rm wildtype.gbk
grep 'tatatata' wildtype.fna
grep 'tatatata' wildtype.fna
grep -c 'tatatata' wildtype.fna
grep -c 'tata' wildtype.fna
grep 'tatatata' wildtype.fna > mutant.fna
sed '1,/^ORIGIN/d' wildtype.gbk | wc -l
wc -l wildtype.gbk
awk '/^LOCUS/{print $3}' wildtype.gbk
awk '/^SOURCE/{print substr($0, 13)}' wildtype.gbk
grep '/gene=' wildtype.gbk | awk -F'=' '{print $2}' | sed 's/"//g'
